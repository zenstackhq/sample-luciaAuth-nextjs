datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

/// @@allow('read', true)
/// @@allow('all', auth() == this)
model User {
  id       String    @id() @default(cuid())
  userName String    @unique()
  /// @omit
  password String
  sessions Session[]
}

model Session {
  id        String   @id()
  userId    String
  expiresAt DateTime
  user      User     @relation(references: [id], fields: [userId], onDelete: Cascade)
}
